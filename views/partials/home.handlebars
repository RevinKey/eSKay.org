      <script type="text/javascript">

        bc(document.getElementsByClassName('video-js')[0]);
        videojs('player').ready(function() {
          var myPlayer = this;
          var playlistDiv = document.getElementsByClassName('vjs-lightbox-playlist')[0];
          myPlayer.on('loadeddata', function() {
            setTimeout(function() {
              var playlist = myPlayer.playlistMenu.items;
              for (var i = 0; i < playlist.length; i++) {
                playlistDiv.appendChild(playlist[i].el());
                playlist[i].el().addEventListener('click', function(i) {
                  return function() {
                    document.getElementById("playerLightbox").className = " ";
                    myPlayer.removeClass('vjs-hidden');
                    document.getElementById("playerClose").className = "playerClose";
                    if (playlist[i].item.longDescription) {
                      document.getElementById("playerClose").innerHTML = '<hr/>' + playlist[i].item.longDescription + '<hr/>';
                    } else {
                      document.getElementById("playerClose").innerHTML = '<hr/>';
                    }
                    document.getElementById("playerLightbox").className = " ";
                  }
                }(i))
              }
            }, 2000);
          })
        })
        //artists section
        var artists = {{{toJSON artists}}};
        $.each(artists.artist, function(index, element){
          let artistsSection = document.getElementById('artists');
          let foundersSection = document.getElementById('founders');
          var cardParent = document.createElement('div');
          cardParent.className = "c-4";
          var card = document.createElement('aside');
          card.className = "artist-card";
          var image = document.createElement('div');
          image.className = "artist-image";
          image.style.backgroundImage = 'url('+ element.image+ ')';
          var icon = document.createElement('i');
          icon.className = "bio-button far fa-id-card";
          image.appendChild(icon);
          var name = document.createElement('div');
          name.className = "artist-name";
          name.innerText = element.name;
          var title = document.createElement('div');
          title.className = "artist-title";
          title.innerText = element.title;
          card.appendChild(title);
          card.appendChild(name);
          card.appendChild(image);
          cardParent.appendChild(card);

          //bio
          var bioNav = document.createElement('div');
          bioNav.className = "sidenav";
          bioNav.id = element.name;
          var close = document.createElement('div');
          close.className = "closebtn";
          close.innerText = 'X';
          var bioImage = document.createElement('img');
          bioImage.src = element.image;
          bioImage.className = "artist-bio-image";
          var bioDiv = document.createElement('div');
          bioDiv.className = "artist-bio";
          var bioName = document.createElement('h1');
          bioName.innerText = element.name;
          var bio = document.createElement('p');
          bio.innerText = element.bio;
          bioDiv.appendChild(bioName);
          bioDiv.appendChild(bio);
          bioNav.appendChild(bioDiv);
          bioNav.appendChild(bioImage);
          bioNav.appendChild(close);

          if (element.title === "Co-Founder") {
            //co-founder
            foundersSection.appendChild(cardParent);
            foundersSection.appendChild(bioNav);


          }else{
            artistsSection.appendChild(cardParent);
            artistsSection.appendChild(bioNav);
          }
          card.addEventListener('click', function() {
            openNav(element.name)
          });
          close.addEventListener('click', function(){
            closeNav(element.name);
          })
        })
        //picture section
        var pictures = {{{toJSON pictures}}};
        var picholder = document.getElementsByClassName('carousel-inner')[0];
        $.each(pictures.sources, function(index, element) {
          var img = document.createElement('img');
          var desc = document.createElement('p');
          desc.innerText = element.desc;
          var imgdiv = document.createElement('div');
          imgdiv.className = "carousel-item";
          img.setAttribute('class', 'd-block lazy1');
          img.setAttribute('data-original', element.src);
          imgdiv.appendChild(img);
          imgdiv.appendChild(desc);
          picholder.appendChild(imgdiv);
        });
        $(function() {
          $(".lazy").lazyload({
            event: "sporty"
          });
        });
        $(window).bind("load", function() {
          var timeout = setTimeout(function() {
            $(".lazy").trigger("sporty")
          }, 3000);
        });
        $(function() {
          $(".lazy1").lazyload({
            event: "sporty1"
          });
        });
        $(window).bind("load", function() {
          var timeout = setTimeout(function() {
            $(".lazy1").trigger("sporty1")
          }, 3000);
        });
        function openNav(id) {
            document.getElementById(id).style.width = "100%";
            document.getElementById(id).style.zIndex = 2000;
        }

        function closeNav(id) {
            document.getElementById(id).style.width = "0";
            document.getElementById(id).style.zIndex = 0;

        }


        function hideAndStop() {
          videojs("player").pause();
          videojs("player").addClass('vjs-hidden');
          document.getElementById("playerLightbox").className = "vjs-hidden";
          document.getElementById("playerClose").className = "vjs-hidden";
        }
        $(document).ready(function() {
          // the body of this function is in assets/js/now-ui-kit.js
            nowuiKit.initSliders();

        });
      </script>
